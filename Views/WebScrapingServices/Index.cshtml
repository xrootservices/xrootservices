	<script>
		document.addEventListener("DOMContentLoaded", function () {
		  const allHeads = document.querySelectorAll(".accordion_head");

		  allHeads.forEach(head => {
			head.addEventListener("click", function () {
			  const body = this.nextElementSibling;
			  const icon = this.querySelector(".plusminus i");
			  const isOpen = body.classList.contains("open");

			  // Close all
			  document.querySelectorAll(".accordion_body").forEach(b => b.classList.remove("open"));
			  document.querySelectorAll(".plusminus i").forEach(i => {
				i.classList.remove("fa-minus");
				i.classList.add("fa-plus");
			  });

			  // Open this one
			  if (!isOpen) {
				body.classList.add("open");
				icon.classList.remove("fa-plus");
				icon.classList.add("fa-minus");
			  }
			});
		  });

		  // Automatically open first FAQ after 3 seconds
		  setTimeout(() => {
			const firstHead = document.querySelector(".accordion_container .accordion_head");
			if (firstHead) {
			  firstHead.click();
			}
		  }, 3000);
		});
	</script>

		

